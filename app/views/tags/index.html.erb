<h1>All Tags</h1>

<ul id="tags">

  <% @articles.each do |tag| %>
    <li>
	  <%= link_to tag.title, tag_path(tag) %>
	</li>
  <% end %>  
  
</ul>

<%= link_to "delete", tag_path(@tag), method: :delete, data: {confirm: "Really delete the tag?"} %>
<%= link_to "Create a New Article", new_article_path, class: "new_article" %>